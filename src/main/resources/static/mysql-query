CREATE TABLE `USER` (
	`USER_SEQ` INT(100) NOT NULL AUTO_INCREMENT COMMENT '회원 번호',
	`USER_ID` VARCHAR(300) NOT NULL COMMENT '회원 ID',
	`PASSWORD` VARCHAR(1000) NOT NULL COMMENT '비밀번호',
	`NAME` VARCHAR(300) NULL DEFAULT NULL COMMENT '회원명',
	`EMAIL` VARCHAR(100) NULL DEFAULT NULL COMMENT '이메일',
	`PHONE` VARCHAR(11) NULL DEFAULT NULL COMMENT '전화번호',
	`CRE_DT` TIMESTAMP NULL DEFAULT NULL COMMENT '생성일',
	`UPD_DT` TIMESTAMP NULL DEFAULT NULL COMMENT '수정일',
	PRIMARY KEY (`USER_SEQ`)
)
COMMENT='회원'
ENGINE=InnoDB
;

