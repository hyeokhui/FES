CREATE TABLE `REFRESH_TOKENS` (
	`USER_ID` VARCHAR(300) NOT NULL COMMENT '유저_아이디',
	`REFRESH_TOKEN` VARCHAR(1000) NOT NULL COMMENT '새로운_토큰',
	PRIMARY KEY (`USER_ID`) USING BTREE
)
COMMENT='새로운_토큰들'
ENGINE=InnoDB
;

CREATE TABLE `ANSWER` (
	`ANS_SEQ` INT(11) NOT NULL AUTO_INCREMENT COMMENT '답변 번호',
	`ANS_CONTENT` VARCHAR(5000) NULL DEFAULT NULL COMMENT '답변 내용',
	`ANS_RATE` DOUBLE NULL DEFAULT NULL COMMENT '답변 별점',
	`DOC_ID` VARCHAR(300) NULL DEFAULT NULL COMMENT '문서 ID',
	`PARAGRAPH_ID` VARCHAR(300) NULL DEFAULT NULL COMMENT '문단 ID',
	`CRE_DT` TIMESTAMP NULL DEFAULT NULL COMMENT '생성일',
	`UPD_DT` TIMESTAMP NULL DEFAULT NULL COMMENT '수정일',
	PRIMARY KEY (`ANS_SEQ`) USING BTREE
)
COMMENT='답변'
ENGINE=InnoDB
;

CREATE TABLE `QUESTION` (
	`QST_SEQ` INT(11) NOT NULL AUTO_INCREMENT COMMENT '질문 번호',
	`ANS_SEQ` INT(11) NULL DEFAULT NULL COMMENT '답변 번호',
	`USER_ID` VARCHAR(300) NULL DEFAULT NULL COMMENT '사용자 ID',
	`QST_CONTENT` VARCHAR(5000) NULL DEFAULT NULL COMMENT '질문 내용',
	`QST_OPINION` VARCHAR(5000) NULL DEFAULT NULL COMMENT '질문 의견',
	`QST_ANS_RATE` DOUBLE NULL DEFAULT NULL COMMENT '별점',
	`CRE_DT` TIMESTAMP NULL DEFAULT NULL COMMENT '생성일',
	`UPD_DT` TIMESTAMP NULL DEFAULT NULL COMMENT '수정일',
	PRIMARY KEY (`QST_SEQ`) USING BTREE
)
COMMENT='질문'
ENGINE=InnoDB
;

CREATE TABLE `ROLE` (
	`ROLE_SEQ` INT(11) NOT NULL AUTO_INCREMENT COMMENT '역할_번호',
	`ROLE_NAME` VARCHAR(50) NOT NULL COMMENT '역할_이름',
	`ROLE_CODE` VARCHAR(50) NOT NULL COMMENT '역할_코드',
	PRIMARY KEY (`ROLE_SEQ`) USING BTREE
)
COMMENT='역할'
ENGINE=InnoDB
;

CREATE TABLE `USER_ROLE` (
	`USER_ROLE_SEQ` INT(11) NOT NULL AUTO_INCREMENT COMMENT '유저_역할_번호',
	`USER_SEQ` INT(11) NOT NULL COMMENT '유저_번호',
	`ROLE_SEQ` INT(11) NOT NULL COMMENT '역할_번호',
	PRIMARY KEY (`USER_ROLE_SEQ`) USING BTREE
)
COMMENT='유저_역할'
ENGINE=InnoDB
;

















